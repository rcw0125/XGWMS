  <DataSets>
    <DataSet Name="XGDataSource">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT barcode, pch, sl, oper, opedate, opetype
FROM WMS_PUB_WorkLoad
WHERE (opedate &gt;= @StartDate) AND (opedate &lt;= @EndDate) AND (oper = @Oper) AND 
      (opetype = @OperType) OR
      (opedate &gt;= @StartDate) AND (opedate &lt;= @EndDate) AND (opetype = @OperType) 
      AND (@Oper = '全部') OR
      (opedate &gt;= @StartDate) AND (opedate &lt;= @EndDate) AND (oper = @Oper) AND 
      (@OperType = '全部') OR
      (opedate &gt;= @StartDate) AND (opedate &lt;= @EndDate) AND (@Oper = '全部') AND 
      (@OperType = '全部')</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Oper">
            <Value>=Parameters!Oper.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@OperType">
            <Value>=Parameters!OperType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>XGDataSource</DataSourceName>
      </Query>
